.warp{
  margin: 8px 20px;
  font-size: 14px;
}
.order-item{
  margin-bottom: 8px;
}
.order-label{
  width: 86px;
}
.order-time{
  text-align: center;
  position: relative;
  height: 247px;
}
.order-time image{
  position: absolute;
  left: 113px;
  top: 50px;
  width: 120px;
  height: 120px;
}
.order-timer{
  position: absolute;
  left: 125px;
  top: 98px;
  width: 91px;
  word-wrap: break-word;
  font-size: 18px;
}
.order-duration{
  position: absolute;
  left: 145px;
  top: 180px;
  color: #666666;
}
.park-info{
  margin-bottom: 20px;
}
.park-title{
  color: #fff;
  height: 35px;
  line-height: 35px;
  text-align: center;
  background-color: rgb(174, 174, 174);
}
.park-content{
  padding: 10px;
  background-color: rgba(242, 242, 242, 1);
}
.park-text{
  font-weight: 600;
  font-family: 'Arial Negreta', 'Arial Normal', 'Arial';
  font-style: normal;
  margin-bottom: 8px;
}