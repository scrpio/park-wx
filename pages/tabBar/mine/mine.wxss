.warp{
  flex-direction: column;
  align-items: center;
  width: 100%;
  height: 100vh;
  background-color: #f4f5fc;
}
.register-btn{
  margin-top: 50%;
  margin-left: 100rpx;
  margin-right: 100rpx;
}
.card{
  color: #fff;
  margin: 20rpx;
  padding-top:30rpx;
  padding-bottom: 30rpx;
  border-radius: 20rpx;
  background-color: #3a81ff;
}
.card-info{
  float: left;
  margin-top: 35rpx;
  margin-left: 20rpx;
}
.card-level{
  float: right;
  color: #ca9311;
  margin-top: 60rpx;
  padding: 10rpx;
  border-radius: 40rpx 0 0 40rpx;
  background-color: #f4ea2a;
}
.card-level image{
  float: left;
  padding-right: 5rpx;
  height: 42rpx;
  width: 42rpx;
}
.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  border-radius: 128rpx;
  float: left;
  margin-top: 30rpx;
  margin-bottom: 60rpx;
  margin-left: 60rpx;
}
.userinfo-nickname {
  font-size: 40rpx;
  font-weight: 550;
}
.userinfo-license {
  font-size: 28rpx;
  margin-top: 20rpx;
}
.card-property{
  display: flex;
  width: 100%;
  text-align: center;
}
.list-item{
  margin: auto;
  text-align: center;
}
.list-item view{
  font-size: 40rpx;
  font-weight: 550;
}
.warp-list{
  overflow: auto;
}
.list{
  font-size: 28rpx;
  height: 100rpx;
  line-height: 100rpx;
  padding-left: 30rpx;
  padding-right: 30rpx;
  background-color: #fff;
  border-bottom: 2rpx solid #e4e4e4;
}
.mt10{
  margin-top: 10rpx;
}
.mb10{
  margin-bottom: 10rpx;
}
.bt{
  border-top: 2rpx solid #e4e4e4;
}
.item-right{
  float: right;
}
.item-icon{
  display: inline-block;
  width: 18rpx;
  height: 30rpx;
  padding-left: 20rpx;
}