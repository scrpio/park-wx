.banner{
  display: flex;
  width: 100%;
}

swiper{
  height: 180px;
  width: 375px;
}
.swiper-item{
  height: 180px;
  width: 375px;
}
.ad{
  width: 100%;
  height: 40px;
  line-height: 40px;
  padding-left: 15px;
  background-color: #ccc;
}
.ad-text{
  font-size: 14px;
}
.horn-img{
  float: left;
  width: 20px;
  height: 20px;
  margin-right: 9px;
  margin-top: 9px;
}
.main{
  display: flex;
  width: 100%;
  text-align: center;
}
.main-left{
  width: 45%;
  text-align: center;
  padding-top:30rpx;
  padding-bottom: 30rpx;
  border-right: 1px solid #eee;
}
.main-right{
  width: 55%;
  text-align: left;
}
.enter-img{
  width: 75px;
  height: 70px;
  margin: auto;
  display: block;
}
.out-img{
  width: 73px;
  height: 56px;
  position: absolute;
  left: 90px;
}
.service-img{
  width: 81px;
  height: 35px;
  position: absolute;
  left: 90px;
}
.item-top{
  position: relative;
  padding:5px 15px;
  height: 56px;
  border-bottom: 1px solid #eee;
}
.item-bottom{
  padding:15px;
  height: 35px;
  position: relative;
}
.item-top label{
  position: absolute;
  top: 27px;
}
.item-bottom label{
  position: absolute;
  top: 22px;
}
.company{
  border-top: 1px solid #eee;
}
.company-bar{
  height: 36px;
  line-height: 36px;
  padding-left: 10px;
}
.company-a{
  float: right;
  font-size: 12px;
  color: #aaa;
}
.company-icon{
  width: 5px;
  height: 10px;
  padding-left: 10px;
  padding-right: 10px;
}
.company-img{
  width: 375px;
  height: 166px;
}