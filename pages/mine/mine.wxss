.warp{
  flex-direction: column;
  align-items: center;
  width: 100%;
  height: 100vh;
  background-color: #f1f1f1;
}
.register-btn{
  margin-top: 50%;
  margin-left: 50px;
  margin-right: 50px;
}
.top-mode{
  height:211px;
  overflow: hidden;
}
.userinfo {
  color: #fff;
  float: left;
  margin-top: 68px;
  margin-left: 15px;
}
.userinfo-avatar {
  width: 64px;
  height: 64px;
  border-radius: 64px;
  float: left;
  margin-top: 68px;
  margin-left: 35px;
}
.userinfo-nickname {
  font-size: 20px;
  font-weight: 550;
}
.userinfo-license {
  font-size: 14px;
  margin-top: 10px;
}
.mine-bar{
  display: flex;
  width: 100%;
  text-align: center;
  border-bottom: 1px solid #e4e4e4;
  background-color: #fff;
}
.list-item{
  margin: auto;
  padding-top:30rpx;
  padding-bottom: 30rpx;
  text-align: center;
}
.warp-list{
  overflow: auto;
}
.list{
  font-size: 14px;
  height: 100rpx;
  line-height: 100rpx;
  padding-left: 30rpx;
  padding-right: 30rpx;
  background-color: #fff;
  border-bottom: 1px solid #e4e4e4;
}
.mt10{
  margin-top: 10rpx;
}
.mb10{
  margin-bottom: 10rpx;
}
.bt{
  border-top: 1px solid #e4e4e4;
}
.item-right{
  float: right;
}
.item-icon{
  display: inline-block;
  width: 9px;
  height: 15px;
  padding-left: 20rpx;
}